
#include <iostream>
#include <math.h> 
using namespace std;

int main()
{
	double a{5.6}, b{1.8}, y, t;
	if (a <= b)
		y = (a - b)/(a + b) * (a + b)/(a * a - a * b + b * b);
	else
y = a + log(b * b);
	if (y == b)
		t = (2 * y + sqrt(y * y - a))/(2 * y - sqrt(a * a - y));
	else if (y < b)
		t = sin(y) * sin(y) + 1/(tan(a - b));
	else

		t = pow(y * sin(a), 1/3)+ 1/(sqrt(y * cos(b))); 
    
	cout << "a = " << a << "\nb = " << b << "\ny = " << y << "\nt = " << t << endl;

	system("pause");
return 0;
}
