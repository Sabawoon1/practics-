#include <iostream>
#include <math.h>
using namespace std;
int main()
{
double a{5.9}, b{-6.9}, n, m;
	if (a >= b)
		n = pow((a - b), 1 / 3);
	else
		n = a * a + (a - b) / sin(a*b);
	if (n < b)

		m = (n + a) / -b + sqrt(sin(a)*sin(a) - cos(n));

	else if (n == b)

		m = b * b + tan(n*a);

	else

		m = b * b * b + n * a * a;

	cout << "a = " << a << "\nb = " << b << "\nn = " << n << "\nm = " << m << endl;

	system("pause");
	return 0;
}
